{
  "manifest_version": 3,
  "name": "Copy Page Title",
  "version": "1.1",
  "description": "One-click copy of the page title.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
